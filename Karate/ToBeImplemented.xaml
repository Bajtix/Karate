<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:karate="clr-namespace:Karate;assembly=Karate"
             x:Class="Karate.ToBeImplemented"
             x:DataType="karate:ToBeImplemented">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center" Margin="20,0">
            
            <Image Source="wip.png" HorizontalOptions="Center"/>
            <Label FontSize="35" HorizontalTextAlignment="Center">Funkcja niedostępna</Label>
            <Label FontSize="Large" HorizontalTextAlignment="Center" x:Name="Msg"></Label>
            
            <Button TextColor="White" Margin="0, 30, 0, 0" Text="eee?" Background="{StaticResource KAccentBrush}" Clicked="GoToAbout" />
            <Button TextColor="White" Text="Zobacz roadmap" Background="{StaticResource KAccentBrush}" Clicked="GoToRoadmap" />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>